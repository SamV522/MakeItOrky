{
    "name": "Make it Orky",
    "description": "An ekstenshun ta translate fings into orky",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
    ],
    "icons": {
        "16": "icons/icon-bitty.png"
    },
    "content_scripts": [
        {
            "matches": ["http://*/*", "https://*/*"],
            "js": ["scripts/makeitorky.js"],
            "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["data/orky.json"],
            "matches": ["http://*/*","https://*/*"]
        }
    ]
}